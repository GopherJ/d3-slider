{
  "name": "d3-slider",
  "version": "0.0.1",
  "description": "Pure D3 slider.",
  "keywords": [
    "d3",
    "slider"
  ],
  "homepage": "https://github.com/MasterMaps/d3-slider",
  "license": "BSD-3-Clause",
  "author": {
    "name": "BjÃ¸rn Sandvik",
    "url": "http://mastermaps.net"
  },
  "main": "build/d3",
  "jsnext:main": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/MasterMaps/d3-slider.git"
  },
  "scripts": {
    "pretest": "mkdir -p build && d3-bundler --polyfill-map --format=umd --name=d3 -- d3.js > build/d3.js",
    "test": "faucet `find test -name '*-test.js'`",
    "prepublish": "npm run test && uglifyjs build/d3.js -c -m -o build/d3.min.js && rm -f build/d3.zip && zip -j build/d3.zip -- LICENSE README.md build/d3.js build/d3.min.js"
  },
  "devDependencies": {
    "d3-scale": "^0.1.1",
    "d3-bundler": "^0.2.7",
    "faucet": "0.0",
    "jsdom": "3",
    "tape": "4",
    "uglifyjs": "2"
  }
}
